{
  "name": "project-setup",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p lint:watch dev:serve",
    "dev:serve": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:fix": "npx eslint --fix \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "format": "npx prettier --write \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "lint": "npx eslint ./src",
    "lint:watch": "npx esw -w --color --clear src --ext .js,.svelte"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.44.0",
    "vite": "^2.8.0"
  }
}
